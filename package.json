{
  "name": "expresso-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build:dev": "webpack",
    "build:prod": "webpack --config webpack.config.prod.js",
    "devserver": "lite-server",
    "watch:sass": "node-sass src/sass/main.scss dist/assets/css/main.css -watch",
    "compile:sass": "node-sass src/sass/main.scss dist/assets/css/main.comp.css",
    "concat:css": "concat -o assets/css/main.concat.css dist/assets/css/main.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" dist/assets/css/main.comp.css -o dist/assets/css/main.prefix.css",
    "compress:css": "node-sass dist/assets/css/main.prefix.css dist/assets/css/main.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.1.0",
    "concat": "^1.0.3",
    "eslint": "^6.8.0",
    "html-webpack-plugin": "^4.0.3",
    "jest": "^26.0.1",
    "lite-server": "^2.5.4",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1"
  }
}
